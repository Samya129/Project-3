    "axios": "^0.18.1",
    "bootstrap": "^4.6.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongodb": "^3.6.6",
    "mongoose": "^5.3.16",
    "react-datepicker": "^3.8.0",
    "react-native-tailwindcss": "^1.1.11"
  },
